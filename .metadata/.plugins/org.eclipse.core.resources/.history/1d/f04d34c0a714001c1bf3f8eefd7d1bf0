	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<parameter name="RESOURCE_DIR" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
rw.FTU_LoadOrder_ID,

	UPPER(cli.name) as grup_emp,
	info.taxid as orif,
	rw.indate as fentrada,
	rw.outdate as fsalida,
	rw.documentno as doc, rw.issotrx ,
	case
		when rw.operationtype = 'DBM' then 'DESPACHO PRODUCTO A GRANEL'
		when rw.operationtype = 'DFP' then 'DESPACHO PRODUCTO TERMINADO'
		when rw.operationtype = 'MOM' then 'MOVIMIENTO DE SALIDA DE MATERIAL'
		when rw.operationtype = 'ORW' then 'OTRAS PESADAS'
		when rw.operationtype = 'RMR' then 'RECEPCION DE MATERIA PRIMA'
		when rw.operationtype = 'PBR' then 'RECEPCION DE PRODUCTO A GRANEL'
		else 'RECEPCION VARIOS PRODUCTOS' end as T_Operacion,
	lorder.DocumentNo  as ordencarga,
	et.documentno as ticket,
	UPPER(ter.name) as tercero,
	UPPER(mshipper.name) as transportista, ftv.vehicleplate,
	UPPER(cond.name) as conductor,
	cond.Value as cedulacond,
	cond.Phone as tlfcond,
	rw.TrailerPlate as placaremolque,
	UPPER(btrim(loc.address1) || coalesce ( ' ' || btrim(loc.address2),
	'' ) || coalesce ( ' ' || btrim(loc.address3),
	'' ) || coalesce ( ' ' || btrim(loc.address4),
	'' )) as direcciondestino,
	prod.value as codp,
	prod.name as producto,
	rw.grossweight as bruto,
	rw.tareweight as tara,
	rw.netweight as neto,
	rw.description as observacion,
	rw.ftu_recordweight_uu as c_verificacion,
	u.name as usuario,
	UPPER(tp.printname)	as tipodoc,rw.Description_alt,
lorder.sealno as precinto,orderline.C_OrderLine_ID ,coalesce (rw.m_product_id ,orderline.m_product_id) m_product_id,
rw.OriginGrossWeight, rw.OriginTareWeight  ,  rw.OriginNetWeight,coalesce(rw.GuideOrigen,' ') as GuideOrigen,
rw.GuideSurvey,rw.GuideSada,rw.TripNumber,rw.TankNumber,rw.RemittanceNumber,anl.documentno as hrs_analysis_N
from ftu_recordweight rw
LEFT OUTER JOIN ad_org ad ON rw.ad_org_id = ad.ad_org_id
LEFT OUTER JOIN ad_orginfo info ON ad.ad_org_id = info.ad_org_id
LEFT OUTER JOIN ad_client cli ON rw.ad_client_id = cli.ad_client_id
left outer join ftu_loadorder lorder on rw.ftu_loadorder_id = lorder.ftu_loadorder_id
left outer join ftu_loadorderline lineaoc on rw.ftu_loadorder_id = lineaoc.ftu_loadorder_id
left outer join FTU_EntryTicket et on rw.ftu_entryticket_id = et.ftu_entryticket_id
left outer join c_orderline orderline on coalesce(et.C_OrderLine_ID,lineaoc.C_OrderLine_ID)  = orderline.C_OrderLine_ID
left outer join c_order orden on orderline.c_order_id  = orden.C_Order_ID
left outer join c_bpartner ter on orderline.c_bpartner_id = ter.c_bpartner_id and orden.c_bpartner_id = ter.c_bpartner_id
left outer join m_shipper mshipper on et.m_shipper_id = mshipper.m_shipper_id
left outer join ftu_driver cond on rw.ftu_driver_id = cond.ftu_driver_id
left join c_bpartner_location loct on orden.c_bpartner_location_id = loct.c_bpartner_location_id 
left join c_location loc on  loct.c_location_id = loc.C_Location_id 
left outer join m_product prod on coalesce (rw.m_product_id ,orderline.m_product_id) = prod.m_product_id
LEFT OUTER JOIN ad_user u ON rw.createdby = u.ad_user_id
left outer join c_doctype tp on rw.c_doctype_id = tp.c_doctype_id
LEFT JOIN ftu_vehicle ftv ON rw.ftu_vehicle_id  = ftv.ftu_vehicle_id
left join HRS_Analysis anl on  rw.ftu_entryticket_id = anl.ftu_entryticket_id
	where
		rw.ftu_recordweight_id = $P{RECORD_ID}
		Limit 1]]>
	</queryString>
