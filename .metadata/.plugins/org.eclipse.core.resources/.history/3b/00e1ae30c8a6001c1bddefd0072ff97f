	<parameter name="C_Currency_ID_To" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1000001]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select 

 "name"	  ,

  case	when name = 'Tipo de Cambio' then
		SUM(amount1)
  else
	currencyconvert(SUM(amount1) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date,0,1000000,0) 
  end as amount1,
	
	
  case	when name = 'Tipo de Cambio' then
  		SUM(amount2)
		else
	currencyconvert(SUM(amount2) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date,0,1000000,0) 
  end as  amount2,

	
   case	when name = 'Tipo de Cambio' then
		SUM(amount3)
		else
	currencyconvert(SUM(amount3) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date,0,1000000,0) 
	end as amount3,
	
   case	when name = 'Tipo de Cambio' then
		SUM(amount4)
		else
	currencyconvert(SUM(amount4) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date,0,1000000,0) 
	end as amount4,
	
 SUM(amount1) as amount1o	  ,
 SUM(amount2) as amount2o	  , 
 SUM(amount3) as amount3o	  ,
 SUM(amount4) as amount4o	  ,  
 to_char( ($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date , 'DD/MM/YYYY') as COL1,
 to_char( ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date , 'DD/MM/YYYY') as COL2,
 to_char( ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date , 'DD/MM/YYYY') as COL3,
 to_char( ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date , 'DD/MM/YYYY') as COL4

,
 case
		when name = 'Tipo de Cambio' then
 (currencyconvert(SUM(amount1) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date,0,1000000,0) +
 currencyconvert(SUM(amount2) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date,0,1000000,0) + 
 currencyconvert(SUM(amount3) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date,0,1000000,0) +
 currencyconvert(SUM(amount4) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date,0,1000000,0) ) / 4 
	else 
  currencyconvert(SUM(amount1) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date,0,1000000,0) +
 currencyconvert(SUM(amount2) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date,0,1000000,0) + 
 currencyconvert(SUM(amount3) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date,0,1000000,0) +
 currencyconvert(SUM(amount4) ,1000001,$P{C_Currency_ID_To},($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date,0,1000000,0)
end as SUMA ,

	case
		when name = 'Tipo de Cambio' then 1.00
		when name = 'Saldo inicial de caja' then 2.00
		when name = 'Cobranzas' then 3.00
		when name = 'Consumo Masivo' then 4.00
		when name = 'Ventas Industriales' then 5.00
		when name = 'INGRESOS' then 6.00
		when name = 'EGRESOS' then 9.01
		
		else 9.00
	end orden, 
	
	case
		when name = 'Tipo de Cambio' then ''
		when name = 'Saldo inicial de caja' then ''
		when name = 'Cobranzas' then ''
		when name = 'Consumo Masivo' then ''
		when name = 'Ventas Industriales' then ''
		when name = 'INGRESOS' then 'B'
		when name = 'EGRESOS' then 'B'
		
		else ''
	end bold
	
	,case
	when $P{C_Currency_ID_To} = 100 then 'Expresado en USD'
	when $P{C_Currency_ID_To} = 1000001 then 'Expresado en VED'
	when $P{C_Currency_ID_To} = 1000000 then 'Expresado en VES'
end expresado
	

	from
((select
	 to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,ar.name,a.amount   as amount1, null amount2, null amount3, null amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between $P{DateTrx} and $P{DateTrx} and a.isactive = 'Y' 
	union
	(
	select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,  'INGRESOS' as name, SUM(a.amount) amount1, 0::numeric as amount2,  0::numeric as amount3, 0::numeric   amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date and a.isactive = 'Y'
	group by 1
	)
	
union 	
	select to_char( $P{DateTrx}::date , 'DD/MM/YYYY') datetrx ,'Tipo de Cambio' as name,
	currencyrate(100,1000001,$P{DateTrx},null,1000000,0) amount1, 0::numeric amount2  , 0::numeric amount3 , 0::numeric amount4)

	union

(select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,ar.name, 0::numeric amount1,a.amount as amount2,  0::numeric amount3,  0::numeric amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date and a.isactive = 'Y'
	
	union 
	(
	select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,  'INGRESOS' as name, 0::numeric amount1,SUM(a.amount) as amount2,  0::numeric amount3,  0::numeric amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date and a.isactive = 'Y'
	group by 1
	)
	
	
union 	
	select to_char( ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx ,'Tipo de Cambio' as name,
	0::numeric amount1  ,
	currencyrate(100,1000001,($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date,null,1000000,0) amount2,
	 0::numeric amount3 , 0::numeric amount4)
	 
	union
	
	(select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,ar.name, 0::numeric amount1, 0::numeric amount2,a.amount as amount3,   0::numeric amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date and a.isactive = 'Y'
	
	union 
	(
	select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,  'INGRESOS' as name, 0::numeric amount1, 0::numeric as amount2,  SUM(a.amount) as amount3,  0::numeric amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date and a.isactive = 'Y'
	group by 1
	)	
	union 	
	select to_char( ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx ,'Tipo de Cambio' as name,
	0::numeric amount1  ,0::numeric amount2 , 
	currencyrate(100,1000001,($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date,null,1000000,0) amount3,
	 0::numeric amount4)
	 
union

(select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,ar.name, 0::numeric amount1, 0::numeric amount2, 0::numeric amount3,a.amount as amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date and a.isactive = 'Y'
union	
	
(
	select
	  to_char(a.datetrx, 'DD/MM/YYYY') datetrx ,  'INGRESOS' as name, 0::numeric amount1, 0::numeric as amount2,  0::numeric as amount3,  SUM(a.amount)  amount4 
from
	COP_DailyOpeningBalance a join ad_ref_list ar on ar.ad_reference_id = 1000176 and a.incometype= ar.value
	
where
	a.DateTrx between ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date and a.isactive = 'Y'
	group by 1
	)
	
	
	
union 	
	select to_char( ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx ,'Tipo de Cambio' as name,
	0::numeric amount1  ,0::numeric amount2 , 0::numeric amount3, 
	currencyrate(100,1000001,($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date,null,1000000,0) amount4
	)
	 
union 

select datetrx	  , "name"	  , sum(amount1) amount1	  , sum(amount2) amount2	  , sum(amount3) amount3	  , sum(amount4) amount4 from 
(
select
to_char( $P{DateTrx}::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,	 
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 )* -1 as amount1  
	,0::numeric as amount2,  0::numeric amount3,  0::numeric amount4 
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between $P{DateTrx} and $P{DateTrx}
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as A1 	
group by datetrx,NAME 

union 

select datetrx	  , "name"	  , sum(amount1) amount1	  , sum(amount2) amount2	  , sum(amount3) amount3	  , sum(amount4) amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	0::numeric as amount1,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount2  
	, 0::numeric amount3,  0::numeric amount4 
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as A1 	
group by datetrx,NAME 

union 
select datetrx	  , "name"	  , sum(amount1) amount1	  , sum(amount2) amount2	  , sum(amount3) amount3	  , sum(amount4) amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	0::numeric as amount1,
	0::numeric as amount2,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount3  
	, 0::numeric amount4 
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as A1 	
group by datetrx,NAME 

union 

(
SELECT
datetrx	  , 'EGRESOS' as name	  , SUM(amount1) as amount1	  , 0::numeric as amount2	  ,   0::numeric  as amount3	  ,   0::numeric  amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount1 ,  
	0::numeric as amount2,
	0::numeric as amount3,
	0::numeric as amount4
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('0 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as XQ
group by datetrx
)



union 

(
SELECT
datetrx	  , 'EGRESOS' as name	  , 0::numeric as amount1	  ,  SUM(amount2) as amount2	  ,  0::numeric  as amount3	  ,   0::numeric  amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	0::numeric as amount1,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount2 ,  
	0::numeric as amount3,
	0::numeric as amount4
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('1 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as XQ
group by datetrx
)


union 

(
SELECT
datetrx	  , 'EGRESOS' as name	  , 0::numeric as amount1	  ,  0::numeric as amount2	  ,SUM(amount3) as amount3	  ,   0::numeric  amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	0::numeric as amount1,
	0::numeric as amount2,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount3 ,  
	0::numeric as amount4
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('2 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as XQ
group by datetrx
)



union 

(

SELECT
datetrx	  , 'EGRESOS' as name	  , 0::numeric as amount1	  ,  0::numeric as amount2	  ,0::numeric as amount3	  , SUM(amount4)  amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	0::numeric as amount1,
	0::numeric as amount2,
	0::numeric as amount3,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount4  
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as XQ
group by datetrx
)




union 
select datetrx	  , "name"	  , sum(amount1) amount1	  , sum(amount2) amount2	  , sum(amount3) amount3	  , sum(amount4) amount4 from 
(
select
to_char( ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date , 'DD/MM/YYYY') datetrx,	 
x1.cop_paygroup_n as name,
	0::numeric as amount1,
	0::numeric as amount2,
	0::numeric as amount3,
	currencyconvert(sum(x1.mount_ved_payment),x1.C_currency_ID,1000001,$P{DateTrx}::date,1000000,1000000,0 ) * -1 as amount4  
from
	cop_paym_shopp x1 left join COP_BPartnerType a on x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID 
where
	x1.DateAcct between ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date and ($P{DateTrx}::date   + CAST('3 days' AS INTERVAL))::date
group by 
x1.C_currency_ID,
x1.cop_paygroup_n
	having sum(x1.mount_ved_payment)  > 0
	order by x1.cop_paygroup_n 
) as A1 	
group by datetrx,NAME ) as X
group by X.NAME 
order by ORDEN ,NAME]]>
