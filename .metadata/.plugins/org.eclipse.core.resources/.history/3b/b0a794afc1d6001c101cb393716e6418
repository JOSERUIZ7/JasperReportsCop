			<textField pattern="#,##0.###;(#,##0.###-)">
				<reportElement x="491" y="0" width="22" height="14" uuid="76256245-63d7-4886-827b-024dd3610614">
					<property name="com.jaspersoft.studio.unit.height" value="px"/>
