	<queryString>
		<![CDATA[SELECT 
a.c_invoice_id,
a.dateinvoiced,    
    c.datetrx AS datepay,

    e.name AS c_bpartner_n,
    e.taxid,
    a.documentno AS c_invoice_n,a.issotrx,
    dt.name AS c_doctype_n,
    a.totallines,
    a.grandtotal,
    c.documentno AS c_payment_n,
    d.overunderamt + d.amount AS amountpay,
    currencyconvert(a.totallines, a.c_currency_id, 100::numeric, a.dateinvoiced::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS totallines_usd,
    currencyconvert(a.grandtotal, a.c_currency_id, 100::numeric, a.dateinvoiced::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS grandtotal_usd,
    currencyconvert(d.amount, c.c_currency_id, 100::numeric, c.datetrx::timestamp with time zone, c.c_conversiontype_id, c.ad_client_id, c.ad_org_id) AS amountpay_usd,
    currencyconvert(d.overunderamt, c.c_currency_id, 100::numeric, c.datetrx::timestamp with time zone, c.c_conversiontype_id, c.ad_client_id, c.ad_org_id) AS overunderamt_usd

    
FROM c_invoice a
     LEFT JOIN c_allocationline b ON b.c_invoice_id = a.c_invoice_id
     LEFT JOIN c_payment c ON c.c_payment_id = b.c_payment_id
     LEFT JOIN c_allocationline d ON d.c_payment_id = c.c_payment_id AND d.c_invoice_id = a.c_invoice_id
     JOIN c_bpartner e ON e.c_bpartner_id = c.c_bpartner_id
     JOIN c_doctype dt ON a.c_doctype_id = dt.c_doctype_id AND (dt.docbasetype = ANY (ARRAY['API'::bpchar, 'APC'::bpchar, 'APD'::bpchar, 'ARI'::bpchar, 'ARD'::bpchar, 'APC'::bpchar])) AND dt.isaffectedbook = 'Y'::bpchar
  WHERE (a.issotrx = 'Y'
  AND a.dateinvoiced between '2023-08-01' AND '2023-08-15')
   or (a.issotrx = 'Y' and c.datetrx between '2023-08-01' AND '2023-08-15') 
  ORDER BY a.dateinvoiced, a.documentno, c.datetrx]]>
	</queryString>
