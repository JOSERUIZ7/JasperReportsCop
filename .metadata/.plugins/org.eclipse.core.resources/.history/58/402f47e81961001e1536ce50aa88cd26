	<parameter name="C_Currency_ID" class="java.lang.Integer"/>
	<queryString language="plsql">
		<![CDATA[select
	case when (NOW() ::DATE-duedate::DATE) < 0 then 'Menor que Cero'  
		 when (NOW() ::DATE-duedate::DATE) between  0 and 7  then '0 - 7'  
		 when (NOW() ::DATE-duedate::DATE) between  8 and 15  then '8 - 15'  
		 when (NOW() ::DATE-duedate::DATE) between 16 and 23  then '16 - 23'  
		 when (NOW() ::DATE-duedate::DATE) between 24 and 31  then '24 - 31'  
		 when (NOW() ::DATE-duedate::DATE) between 32 and 999999999  then 'Mayor a 31' end defeated,  
		a.name COP_PayGroup_n,

	 case 
	 	when $P{C_Currency_ID} = 1000001 then 
			currencyconvert(invoiceopen(c_invoice_id, 0), i.C_Currency_ID, 1000001, i.dateinvoiced::timestamp with time zone, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id) 
	 	when $P{C_Currency_ID} = 100 then 
   			currencyconvert(invoiceopen(c_invoice_id, 0), i.C_Currency_ID, 100, i.dateinvoiced::timestamp with time zone, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id)
   		else 0 
   	 end AS invoiceopen,
   	 'Analisis de Vencimientos de CxP' reponame ,
  	case 
		when $P{C_Currency_ID} = 1000001 then 
	 		'Montos Expresados en Bs'
	 	when $P{C_Currency_ID} = 100 then 
		 	'Montos Expresados en Dolares'
	 	else ''
 	end	 as doc_info 
from cop_rv_c_invoice  i left join COP_PayGroup a on a.COP_PayGroup_ID = i.COP_PayGroup_ID 
where 
DocStatus = 'CO' and issotrx = 'N'
order by 1,2
]]>
	</queryString>
