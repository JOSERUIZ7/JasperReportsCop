		<![CDATA[
select
	distinct 
	A.DOCUMENTNO,A.DateInvoiced,
	b.name,	 
	A.TotalLines ,
	A.GrandTotal, 
	1333.9044 as VALOR , 
	1333.9044 - A.TotalLines as ajuste 

from
	c_invoice a join C_BPARTNER B on B.C_BPARTNER_ID = A.C_BPARTNER_ID
where
	a.C_Invoice_ID=  1016792 ]]>
