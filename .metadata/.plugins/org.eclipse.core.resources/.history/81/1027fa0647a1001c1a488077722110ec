	<queryString language="plsql">
		<![CDATA[--RPT_Account_Status_Report.jrxml

select
a.ad_org_id ,
	c.taxid ,
	c.name c_bpartner_n,
	c.SO_CreditLimit,
	a.documentno ,
	a.datedoc,
	coalesce(A.DUEDATE, A.DATEDOC) as DUEDATE,
	A.netdays,
	d.name C_PaymentTerm_n,
	case
		when b.docbasetype = 'ARI' and upper(b.name) like '%FACTURA%' then 'Factura'
		when b.docbasetype = 'ARI' and upper(b.name) like '%DEBITO%' then 'NotaDebito'
		when b.docbasetype = 'ARI' and upper(b.name) like '%DÉBITO%' then 'NotaDebito'
		when b.docbasetype = 'ARI' and upper(b.name) like '%N/D%' then 'NotaDebito'
		when b.docbasetype = 'ARR' then 'Pagos'
		when b.docbasetype = 'ARC' and upper(b.name) like '%CREDITO%'  then 'NotaCredito'
		when b.docbasetype = 'ARC' and upper(b.name) like '%CRÉDITO%'  then 'NotaCredito'
		when b.docbasetype = 'ARC' and upper(b.name) like '%N/C%'  then 'NotaCredito'
	end as tipo ,
	b.name,
	b.docbasetype ,
	A.C_Currency_ID,F.ISO_Code,
	a.amt GrandTotal_orIG,
	a.openamt openamt_ORIG ,
	currencyconvert(a.amt    , A.C_Currency_ID,1000000 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS GrandTotal,
	currencyconvert(a.cop_openamt, A.C_Currency_ID,1000000 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS openamt,
	currencyconvert(a.amt    , A.C_Currency_ID,100 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS GrandTotal2,
	currencyconvert(a.cop_openamt, A.C_Currency_ID,100 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS openamt2,
	currencyconvert(1    , 100,1000000 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id) AS valdiv,
	csr.valor region_v ,csr.name region_n,coalesce(CB2.NAME,AU.NAME) as salesrep_N,
'Informe de Estado de Cuenta' reponame,
	case when $P{C_BPartner_ID} is null then '' else ' Solo tercero: ' || c.name || chr(13) ||  ' - '  end  ||
	case when $P{AD_Org_ID}     is null then '' else ' Solo organizacion: ' || org.name || chr(13) || ' - ' end ||
	case when $P{C_SalesRegion1}  is null then '' else ' Desde Region de ventas: ' || coalesce(csr.name,'')  || ' - ' end || 
	case when $P{C_SalesRegion2}  is null then '' else ' Hasta Region de ventas: ' || coalesce(csr.name,'')  || ' - ' end 
	 as doc_info,
	case when $P{Range1}  is null then '' else 'Saldo >=  ' || coalesce($P{Range1} ::text ,'')  || ' ' end || 
	case when $P{Range2}  is null then '' else '/ Saldo <= ' || coalesce($P{Range2} ::text ,'')  || ' - ' end as rang_value
from
	cop_rv_bpartneropen a
     join c_doctype b on	a.c_doctype_id = b.c_doctype_id
     join c_bpartner c on a.c_bpartner_id = c.c_bpartner_id
left join C_PaymentTerm d on a.C_PaymentTerm_ID = d.C_PaymentTerm_ID
     join c_currency f on a.c_currency_id = f.c_currency_id
left join cop_c_salesregion csr on a.C_SalesRegion_ID = csr.C_SalesRegion_ID
join ad_org org on a.ad_org_id = org.ad_org_id 
left join AD_User AU on AU.AD_User_ID = CSR.salesrep_id
left join c_bpartner cb2 on CB2.C_bpartner_ID = AU.C_bpartner_ID 
where
	a.issotrx = $P{IsSOTrx}  and a.docstatus = 'CO'
   and (a.C_BPartner_ID = $P{C_BPartner_ID} or  $P{C_BPartner_ID} is null)
   and (a.AD_Org_ID = $P{AD_Org_ID} or $P{AD_Org_ID} is null) 
   
    and ( csr.valor >= $P{C_SalesRegion1}::numeric or $P{C_SalesRegion1} is null) and 
     ( csr.valor <= $P{C_SalesRegion2}::numeric or $P{C_SalesRegion2} is null)
     
 and ( (currencyconvert(a.cop_openamt, A.C_Currency_ID,1000000 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id)
>= $P{Range1} or $P{Range1} is null )  and 
(currencyconvert(a.cop_openamt, A.C_Currency_ID,1000000 , a.datedoc::timestamp with time zone, a.c_conversiontype_id, a.ad_client_id, a.ad_org_id)
<= $P{Range2} or $P{Range2} is null ))

     
 order by
		csr.name,c.name,a.datedoc,A.DOCUMENTNO]]>
	</queryString>
