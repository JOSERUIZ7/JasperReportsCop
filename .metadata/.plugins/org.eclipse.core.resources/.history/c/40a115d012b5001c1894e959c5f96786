	<parameter name="C_Currency_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select 
		ad_client_id ,
		ad_org_id ,
		fiscalyear ,
		periodo ,
		fecha ,
		tipo_operacion ,
		"name" ,
		c_currency_id ,
		MONTO as MONTOORI ,
		currencyconvert(MONTO,c_currency_id,$P{C_Currency_ID}::NUMERIC,dateacct,1000000,1000000,0) SUM ,
		perfecha ,
		dateacct

from 
(
SELECT  
	ad_client_id, ad_org_id, FiscalYear, PERIODO, fecha, tipo_operacion, name, c_currency_id, 
	SUM(amount) MONTO ,trim(PERIODO) || chr(13) || trim(fecha) as perfecha,dateacct::DATE
FROM
v_cop_monthlycashflow
where fiscalyear = $P{FiscalYear}
GROUP BY 1,2,3,4,5,6,7,8,dateacct
ORDER BY 3,4,5,6)
as X]]>
	</queryString>
