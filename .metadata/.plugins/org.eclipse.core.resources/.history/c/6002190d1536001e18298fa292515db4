	<queryString>
		<![CDATA[select
	distinct 
	a.DOCUMENTNO,
	a.DateInvoiced,
	max(ct.Rate) / 100 + 1 as rate ,
	b.name,	 
	a.TotalLines ,
	a.GrandTotal, 
	currencyrate(a.c_currency_id,
	1000001,
	a.dateinvoiced,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id) tasa,
	236559 as VALOR , 
	236559 - a.TotalLines as ajuste 
,
	a.TotalLines,
	((currencyconvert(a.GrandTotal,
	a.c_currency_id,
	1000001,
	a.dateinvoiced,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id)) / (max(ct.Rate) / 100 + 1) ) as diferencia2,
	236559 - ((currencyconvert(a.GrandTotal,
	a.c_currency_id,
	1000001,
	a.dateinvoiced,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id)) / (max(ct.Rate) / 100 + 1) ) as ajuste2 ,
	currencyconvert(236559 - ((currencyconvert(a.GrandTotal,
	a.c_currency_id,
	1000001,
	a.dateinvoiced,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id)) / 1.16 ),
	1000001,
	a.c_currency_id,
	a.dateinvoiced,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id) as ajuste3, 
	(currencyconvert(a.GrandTotal,
	a.c_currency_id,
	1000001,
	a.dateinvoiced,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id)) valor2
from
	c_invoice a
join C_BPARTNER B on
	B.C_BPARTNER_ID = a.C_BPARTNER_ID
join C_InvoiceTax c on
	c.c_invoice_id = a.c_invoice_id
join c_tax ct on
	ct.c_tax_id = c.C_Tax_ID
where
	a.C_Invoice_ID = 1051604
group by
	a.documentno,
	a.dateinvoiced,
	b.name,
	a.totallines,
	a.grandtotal,
	a.c_currency_id,
	a.c_conversiontype_id,
	a.ad_client_id,
	a.ad_org_id]]>
	</queryString>
