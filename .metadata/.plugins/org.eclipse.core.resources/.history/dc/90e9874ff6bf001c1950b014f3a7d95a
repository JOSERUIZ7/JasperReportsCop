			join ad_ref_list ar on
						ar.ad_reference_id = 1000176
				and a.incometype = ar.value
			where
						a.DateTrx between TO_DATE('01/02/' ||  to_char($P{YearTrx},'9999')   , 'DD/MM/YYYY')::date and 
						(TO_DATE('01/03/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') - CAST('1 days' AS INTERVAL))::date
						and a.isactive = 'Y' and a.c_currency_id = $P{C_Currency_ID}
			group by 1)
			
	union 
					(select
							max(t_ope) t_ope ,
							max(datetrx1) datetrx1,
								max(datetrx2) datetrx2 ,
							"name" ,
							0 amount1 ,
							0 amount2 ,
							0 amount3 ,
							sum(amount) amount4 ,
							0 amount5  ,
							0 amount6  ,
							0 amount7  ,
							0 amount8  ,
							0 amount9  ,
							0 amount10 ,
							0 amount11 ,
							0 amount12 
						from
							(
							select
								 40 t_ope ,
								TO_DATE('01/02/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') datetrx1 ,
								(TO_DATE('01/03/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') - CAST('1 days' AS INTERVAL))::date datetrx2 ,
								x1.dateacct,x1.C_currency_ID,
								x1.group_parent as name,(x1.mount_payment),
								x1.mount_payment as amount,							
								case when x1.C_Charge_ID = 500307 then 'Y' else 'N' end transferencia
							from
								cop_paym_shopp x1
							left join COP_BPartnerType a on
								x1.COP_BPartnerType_ID = a.COP_BPartnerType_ID
							where
								x1.DateAcct between TO_DATE('01/02/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') and (TO_DATE('01/03/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') - CAST('1 days' AS INTERVAL))::date
								and x1.c_currency_id = $P{C_Currency_ID}
							order by x1.group_parent ,x1.dateacct
							) as A1
								where a1.transferencia = 'N'
	
							group by
								datetrx1,NAME)
