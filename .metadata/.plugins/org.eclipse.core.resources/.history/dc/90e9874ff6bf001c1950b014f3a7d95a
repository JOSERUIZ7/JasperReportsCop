						ar.ad_reference_id = 1000176
				and a.incometype = ar.value
			where
						a.DateTrx between TO_DATE('01/07/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY')::date and 
						TO_DATE('31/07/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY')
