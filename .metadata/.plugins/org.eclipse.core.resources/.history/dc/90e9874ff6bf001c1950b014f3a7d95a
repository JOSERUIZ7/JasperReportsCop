				a.DateTrx between TO_DATE('01/07/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') and  TO_DATE('31/07/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY')
				and a.isactive = 'Y' and a.c_currency_id = $P{C_Currency_ID}
			group by
				a.datetrx,
				ar.name)
	union
			(select
						30 t_ope ,
						TO_DATE('01/07/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') datetrx1 ,  
						TO_DATE('31/07/' || to_char($P{YearTrx},'9999')    , 'DD/MM/YYYY') datetrx2 ,  
