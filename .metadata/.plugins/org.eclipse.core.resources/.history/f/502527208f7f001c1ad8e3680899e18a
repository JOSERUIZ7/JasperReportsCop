	<queryString>
		<![CDATA[select  distinct 
   ROW_NUMBER() OVER (ORDER BY cb.name ) as enumerar,
   ar.IsWasPresent,
   cb.name,
  cb.taxid,
   cb.Qty,
   cb.Qty*100/(select sum(a.Qty) from c_bpartner a where a.IsShareHolder='Y' and a.isactive='Y') as margen,
   us.name,
   '' as Firma,
   '' as Dactilar
   
   from 
   c_bpartner cb
   left join cop_rv_shareholder sh on cb.c_bpartner_id = sh.c_bpartner_id
   join COP_AssemblyRecordLine ar on cb.c_bpartner_id = ar.c_bpartner_id
   left join ad_user us on cb.c_bpartner_id = us.c_bpartner_id and us.IsLegalRep = 'Y' and  ar.ad_user_id=us.ad_user_id
   
   where 
  cb.IsShareHolder = 'Y'
   and sh.isactive = 'Y'
   and ar.IsWasPresent = 'Y'
   and ar.COP_AssemblyRecord_id=$P{COP_AssemblyRecord_ID}
  order by cb.name]]>
	</queryString>
