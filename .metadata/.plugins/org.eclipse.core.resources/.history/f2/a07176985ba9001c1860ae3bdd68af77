	<queryString language="plsql">
		<![CDATA[select 
c_payment_id ,
documentno , MAX(CB.NAME) as c_bpartner_n , MAX(CB.TAXID) as  TAXID,
dateacct ,
null duedate,
datetrx datedoc ,
X.c_doctype_id , MAX(D.NAME) as C_DocType_N,
X.c_bpartner_id ,
currencyconvert( payamt  , currency_doc , $P{C_Currency_ID},$P{DateTrx},0,1000000,0) as grandtotal ,
currency_doc ,
--SUM(mont_asig) mont_asig,
currencyconvert( SUM(mont_asig) , currency_doc , $P{C_Currency_ID},$P{DateTrx},0,1000000,0) payment , 
--payamt - SUM(mont_asig) as DIF
currencyconvert( payamt  , currency_doc , $P{C_Currency_ID},$P{DateTrx},0,1000000,0) - currencyconvert( SUM(mont_asig) , currency_doc , $P{C_Currency_ID},$P{DateTrx},0,1000000,0) as openamt
,MAX(D1.VALUE) D1,MAX(D1.NAME) DN1
,MAX(H1.VALUE) H1 ,MAX(H1.NAME) HN1,max(CTAD) CTAD2,max(CTAh) CTAh2 ,'' region_n, ''salesrep_n, max(cc.ISO_Code) as ISO_CODE,  'Expresado en ' || max(cc.ISO_Code) as current_to_n, 'Informe de Estado de Cuenta por Anticipos sin Asignar a la Fecha' as reponame,
'anticipo' tipo ,'' c_paymentterm_n,
	case when $P{C_BPartner_ID} is null then '' else ' Solo tercero: ' || max(cb.name) || chr(13) ||  ' - '  end  ||
	case when $P{AD_Org_ID}     is null then '' else ' Solo organizacion: ' || max(org.name) || chr(13) || ' - ' end 
	 as doc_info
from (
			
			select
				a.C_Payment_id,
				a.documentno,
				a.DateAcct,
				a.DateTrx,
				a.c_doctype_id ,
				a.c_bpartner_id,
				a.PayAmt,
				a.c_currency_id currency_doc ,a.ad_org_id ,
				(coalesce(abs(b.Amount),0)) mont_asig1 ,
				(coalesce(currencyconvert(coalesce(abs(b.Amount),0),c.c_currency_id,a.c_currency_id,c.datetrx, null,c.ad_client_id,c.ad_org_id),0)) mont_asig,
				a.PayAmt - (coalesce(currencyconvert(coalesce(abs(b.Amount),0),c.c_currency_id,a.c_currency_id,c.datetrx, null,c.ad_client_id,c.ad_org_id),0)) saldo,
				c.c_currency_id currency_pay , c.datetrx date_pay	,x.CTAD,x.CTAh
			from
				C_Payment a left join C_AllocationLine b on a.C_Payment_id = b.C_Payment_id 
				left join C_AllocationHdr c on c.c_allocationhdr_id  = b.c_allocationhdr_id and c.datetrx <=  $P{DateTrx}
				left join 
				 
						(
						select C_Payment_ID,STRING_AGG(ctad::text,' / ')	ctad  , STRING_AGG(ctah::text,' / ') ctah from 
							(select max(record_id) record_id,c.C_Payment_ID,
								case when AmtSourceDr > 0 THEN
								STRING_AGG (B.VALUE ||B.NAME::TEXT ,' / ')  end CTAD, 
							
								case when AmtSourceCr > 0 THEN
								STRING_AGG (B.VALUE ||B.NAME::TEXT ,' / ')  end CTAH from
								Fact_Acct a join C_ElementValue B on A.account_id = B.c_elementvalue_id
								left join C_AllocationLine c on c.C_AllocationHdr_ID = a.record_id
							where
							record_id  in 
								(select C_AllocationHdr_ID from C_AllocationLine
--								where C_Payment_ID in (1021666,1021667) 
								) and ad_table_id = 735
							group by c.C_Payment_ID,AmtSourceCR,AmtSourceDr) as x group by  x.C_Payment_ID
													) x on x.C_Payment_ID = a.C_Payment_ID
			where
				a.isactive = 'Y'
				and a.isreceipt = $P{IsReceipt}
				and A.DOCSTATUS = 'CO'
				and A.c_doctype_id not in (1000105,1000303)
--				and a.C_Payment_ID = 1021666
			--	and A.C_BPartner_ID = 1013272
		   and (a.C_BPartner_ID = $P{C_BPartner_ID} or  $P{C_BPartner_ID} is null)
		   and (a.AD_Org_ID = $P{AD_Org_ID} or $P{AD_Org_ID} is null) 
				
				and a.DateAcct <= $P{DateTrx}

)
	as X left join c_bpartner cb on X.c_bpartner_id = CB.c_bpartner_id
	left join (select
					A.record_id ,				
					A.account_id ,
					B.VALUE,
					B.NAME
				from
					Fact_Acct A
				join C_ElementValue B on
					A.account_id = B.c_elementvalue_id
				where
					 A.AD_TABLE_ID = 335
					and AmtAcctDr <> 0
			) as D1 on D1.record_id = X.c_payment_id 
			
	left join (select
					A.record_id ,				
					A.account_id ,
					B.VALUE,
					B.NAME
				from
					Fact_Acct A
				join C_ElementValue B on
					A.account_id = B.c_elementvalue_id
				where
					 A.AD_TABLE_ID = 335
					and AmtAcctCr <> 0
			) as H1 on H1.record_id = X.c_payment_id 
			join C_DocType D on D.C_DocType_ID = X.C_DocType_ID
			join c_currency cc on cc.c_currency_id = $P{C_Currency_ID}
		   join ad_org org on org.ad_org_id = x.ad_org_id 
group by 
	X.C_Payment_id,
	X.documentno,
	X.DateAcct,
	X.DateTrx,
	X.c_doctype_id ,
	X.c_bpartner_id,
	X.PayAmt,X.currency_doc
	having currencyconvert( payamt  , currency_doc , $P{C_Currency_ID},$P{DateTrx},0,1000000,0) - currencyconvert( SUM(mont_asig) , currency_doc , $P{C_Currency_ID},$P{DateTrx},0,1000000,0)  <> 0
	order by 3,1,5

	
	
	]]>
	</queryString>
