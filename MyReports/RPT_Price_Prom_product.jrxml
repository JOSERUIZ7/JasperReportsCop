	<queryString language="plsql">
		<![CDATA[select
	R1.m_product_n ,
	max(r1.IsIndustrialSales) IsIndustrialSales ,
	max(r1.ismassiveconsume) ismassiveconsume,
	r1.FTU_ProductGroup_n,
	MAX(R1.WeightSunagro) Weight,
	SUM(R1.qtyinvoiced) qtyinvoiced , 
	max(r1.sum_qty) sum_qty ,
	case
		when max(r1.sum_qty)  <> 0 then
		SUM(R1.qtyinvoiced) / max(r1.sum_qty) * 100
		else
	  0
	end as PORC_PESO,
	(SUM(R1.qtyinvoiced) * MAX(R1.WeightSunagro)) as CANTWEIGHT,
	SUM(R1.linenetamt_cv) linenetamt_cv ,
	SUM(R1.total_line_cv) total_line_cv ,
	case
		when SUM(R1.qtyinvoiced) <> 0 then
		round((SUM(R1.linenetamt_cv) / SUM(R1.qtyinvoiced)), 2)
		else
	  0
	end as prom_linenet_cv,
	case
		when SUM(R1.qtyinvoiced) <> 0 then
		round((SUM(R1.total_line_cv) / SUM(R1.qtyinvoiced)), 2)
		else 
	  0
	end
		as prom_total_cv,	
	
	case
		when MAX(R1.WeightSunagro) = 0
		or MAX(R1.WeightSunagro) isnull
		or SUM(R1.qtyinvoiced) = 0
	then 0
		else round((SUM(R1.linenetamt_cv) / SUM(R1.qtyinvoiced)) / MAX(R1.WeightSunagro), 2)
	end as p1 ,	

	
	case
		when MAX(R1.WeightSunagro) = 0
		or MAX(R1.WeightSunagro) isnull
		or SUM(R1.qtyinvoiced) = 0 or max(r1.sum_qty) = 0
	then 0
		else 
		round((SUM(R1.linenetamt_cv) / SUM(R1.qtyinvoiced)) / MAX(R1.WeightSunagro) * (SUM(R1.qtyinvoiced) / max(r1.sum_qty) ) , 2)
	end as PROMP1,
	
	case
		when SUM(R1.qtyinvoiced) <> 0 and max(r1.sum_qty) <> 0 then 
		round( (SUM(R1.linenetamt_cv) / SUM(R1.qtyinvoiced)) * (SUM(R1.qtyinvoiced) / max(r1.sum_qty) ) , 2)
		else 
		0
	end as PROMLINE ,	
	 R1.reponame ,
	R1.doc_info,
case when 	r1.sum_qty_all <> 0 then 
 (SUM(R1.qtyinvoiced) / r1.sum_qty_all) *  
 
 	case
		when SUM(R1.qtyinvoiced) <> 0 then
		round((SUM(R1.total_line_cv) / SUM(R1.qtyinvoiced)), 2)
		else 
	  0
	end 
end var1	,max(iso_code) iso_code
from
	(
	select
		(SUM(a.qtyinvoiced) over(partition by fpg."name")) sum_qty,
		(SUM(a.qtyinvoiced) over( )) sum_qty_all,
		a.qtyinvoiced,
		D.WeightSunagro,
		d.IsIndustrialSales,
		d.ismassiveconsume ,
		ROUND(currencyConvert(a.linenetamt , a.c_currency_id, $P{C_Currency_ID}, a.dateinvoiced, a.C_ConversionType_ID, a.AD_CLIENT_ID, a.AD_ORG_ID ), 2) as linenetamt_cv,
		ROUND(currencyConvert(((a.linenetamt + a.linenetamt * (a.rate / 100::numeric))), a.c_currency_id, $P{C_Currency_ID}, a.dateinvoiced, a.C_ConversionType_ID, a.AD_CLIENT_ID, a.AD_ORG_ID ), 2) as total_line_cv,
		trim(d.value) || ' - ' || d.name m_product_n,
		fpg."name" FTU_ProductGroup_n,
		(
		case
			when a.DocBaseType in ('ARI', 'ARC') then 'Precios promedios por Producto'
			when a.DocBaseType in ('API', 'APC') then 'Precios promedios por Producto'
		end) REPONAME ,
		'Montos expresados en: ' || cu.Description || CHR(13) || 'DESDE: ' || SUBSTR($P{DateInvoiced1},
		9,
		2) || '/' || SUBSTR($P{DateInvoiced1},
		6,
		2) || '/' || SUBSTR($P{DateInvoiced1},
		1,
		4) || ' - HASTA:' || SUBSTR($P{DateInvoiced2},
		9,
		2) || '/' || SUBSTR($P{DateInvoiced2},
		6,
		2) || '/' || SUBSTR($P{DateInvoiced2},
		1,
		4) || CHR(13) || ' Estado documento:' || st.name || chr(13) ||
		case
			when $P{C_BPartner_ID} is null then ''
			else ' Solo tercero: ' || b.name || ' - '
		end ||
		case
			when $P{AD_Org_ID} is null then ''
			else ' Solo organizacion: ' || org.name || ' - '
		end ||
		case
			when $P{C_DocType_ID} is null then ''
			else ' Solo Documentos: ' || cd.name || ' - '
		end ||
		case
			when $P{C_BP_Group_ID} is null then ''
			else ' Solo Grupo de terceros: ' || bpg.name || ' - '
		end ||
		case
			when $P{M_Product_Category_ID} is null then ''
			else ' Solo Categoria de productos: ' || mpc.name || ' - '
		end ||
		case
			when $P{M_Product_ID} is null then ''
			else ' Solo Producto: ' || d.name
		end ||
		case
			when $P{IsIndustrialSales} = 'Y' then ' -Ventas Industriales- '
			else ''
		end || 
		case
			when $P{IsMassiveConsume} = 'Y' then ' -Consumo Masivo- '
			else ''
		end		
as doc_info,cu.iso_code || chr(13) || ' Unidades' as iso_code
	from
		cop_rv_c_invoiceline a
	join c_bpartner b on
		a.c_bpartner_id = b.c_bpartner_id
	left join m_product d on
		a.m_product_id = d.m_product_id
	join ad_org org on
		a.ad_org_id = org.ad_org_id
	join c_doctype cd on
		a.C_DocType_ID = cd.C_DocType_ID
		and a.C_DocType_ID in

		(
		select
						C_DocType_id
		from
						C_DocType
		where
						name like '%N/C Dev. %'
			and NAME not like '%Donaci%'
			and NAME not like '%Dotaci%'
	union
		select
						C_DocType_id
		from
						C_DocType
		where
						C_DocTypeDeclare = '01'
			and NAME not like '%Donaci%'
			and NAME not like '%Dotaci%'
					)
	join C_Currency CU on
		$P{C_Currency_ID} = cu.C_Currency_ID
	join C_BP_Group bpg on
		a.C_BP_Group_ID = bpg.C_BP_Group_ID
	join M_Product_Category mpc on
		a.M_Product_Category_ID = mpc.M_Product_Category_id
	join (
		select
			a.value,
			b."name"
		from
			ad_ref_list a
		join AD_Ref_List_Trl b on
			a.ad_ref_list_id = b.ad_ref_list_id
		where
			a.ad_reference_id = 131
			and b.ad_language = 'es_CO') as st on
		st.value = $P{DocStatus}
	left join FTU_ProductGroup fpg on
		d.ftu_productgroup_id = fpg.ftu_productgroup_id
	where
		a.IsSOTrx = $P{IsSOTrx}
		and A.AD_CLIENT_ID = 1000000
		
		and (a.IsIndustrialSales = $P{IsIndustrialSales}
			or $P{IsIndustrialSales} is null)
			
		and (a.IsMassiveConsume = $P{IsMassiveConsume}
			or $P{IsMassiveConsume} is null)
			
			
		and (a.C_DocType_ID = $P{C_DocType_ID}
			or $P{C_DocType_ID} is null)
		and a.dateinvoiced between $P{DateInvoiced1} and $P{DateInvoiced2}
		and a.docstatus = $P{DocStatus}
		and (a.C_BPartner_ID = $P{C_BPartner_ID}
			or $P{C_BPartner_ID} is null)
		and (a.AD_Org_ID = $P{AD_Org_ID}
			or $P{AD_Org_ID} is null)
		and (a.C_BP_Group_ID = $P{C_BP_Group_ID}
			or $P{C_BP_Group_ID} is null)
		and (a.M_Product_Category_ID = $P{M_Product_Category_ID}
			or $P{M_Product_Category_ID} is null)
		and (a.SalesRep_ID = $P{SalesRep_ID}
			or $P{SalesRep_ID} is null)
		and (a.M_Product_ID = $P{M_Product_ID}
			or $P{M_Product_ID} is null)
	order by
		fpg."name" )as R1
group by
	R1.m_product_n ,
	r1.FTU_ProductGroup_n,
	R1.reponame ,
	R1.doc_info,r1.sum_qty_all
order by

	r1.FTU_ProductGroup_n,
	m_product_n]]>
	</queryString>
